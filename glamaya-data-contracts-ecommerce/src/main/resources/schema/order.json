{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Glamaya E-Commerce Order",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "orderNumber": {
      "type": "string"
    },
    "orderNumberString": {
      "type": "string"
    },
    "dateCreated": {
      "type": "string",
      "format": "date-time"
    },
    "buyerInfo": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "identityType": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "contactID": {
          "type": "string"
        }
      }
    },
    "currency": {
      "type": "string"
    },
    "weightUnit": {
      "type": "string"
    },
    "totals": {
      "type": "object",
      "properties": {
        "subtotal": {
          "type": "string"
        },
        "shipping": {
          "type": "string"
        },
        "tax": {
          "type": "string"
        },
        "discount": {
          "type": "string"
        },
        "total": {
          "type": "string"
        },
        "weight": {
          "type": "string"
        },
        "quantity": {
          "type": "integer"
        },
        "refund": {
          "type": "string"
        }
      }
    },
    "billingInfo": {
      "type": "object",
      "properties": {
        "paymentMethod": {
          "type": "string"
        },
        "paymentGatewayTransactionId": {
          "type": "string"
        },
        "address": {
          "$ref": "contact.json#/definitions/address"
        },
        "paidDate": {
          "type": "string",
          "format": "date-time"
        },
        "externalTransactionId": {
          "type": "string"
        },
        "paymentProviderTransactionId": {
          "type": "string"
        }
      }
    },
    "shippingInfo": {
      "type": "object",
      "properties": {
        "deliveryOption": {
          "type": "string"
        },
        "estimatedDeliveryTime": {
          "type": "string"
        },
        "shippingRegion": {
          "type": "string"
        },
        "code": {
          "type": "string"
        },
        "shipmentDetails": {
          "type": "object",
          "properties": {
            "address": {
              "$ref": "contact.json#/definitions/address"
            },
            "trackingInfo": {
              "$ref": "#/definitions/trackingInfo"
            },
            "discount": {
              "type": "string"
            },
            "tax": {
              "type": "string"
            },
            "priceData": {
              "$ref": "#/definitions/lineItemPriceData"
            }
          }
        }
      }
    },
    "read": {
      "type": "boolean"
    },
    "archived": {
      "type": "boolean"
    },
    "paymentStatus": {
      "type": "string"
    },
    "fulfillmentStatus": {
      "type": "string"
    },
    "lineItems": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/lineItem"
      }
    },
    "activities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "author": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      }
    },
    "fulfillments": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "dateCreated": {
            "type": "string",
            "format": "date-time"
          },
          "lineItems": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/lineItem"
            }
          },
          "trackingInfo": {
            "$ref": "#/definitions/trackingInfo"
          }
        }
      }
    },
    "cartID": {
      "type": "string"
    },
    "channelInfo": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        }
      }
    },
    "enteredBy": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "identityType": {
          "type": "string"
        }
      }
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "numericID": {
      "type": "string"
    },
    "refunds": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "dateCreated": {
            "type": "string",
            "format": "date-time"
          },
          "amount": {
            "type": "string"
          },
          "reason": {
            "type": "string"
          },
          "isManual": {
            "type": "boolean"
          },
          "refundID": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "externalRefund": {
            "type": "boolean"
          }
        }
      }
    },
    "isInternalOrderCreate": {
      "type": "boolean"
    },
    "buyerLanguage": {
      "type": "string"
    },
    "invoiceInfo": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "source": {
          "type": "string"
        }
      }
    },
    "discount": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "value": {
          "type": "string"
        },
        "appliedCoupon": {
          "type": "object",
          "properties": {
            "couponID": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "code": {
              "type": "string"
            }
          }
        }
      }
    },
    "buyerNote": {
      "type": "string"
    }
  },
  "definitions": {
    "fullName": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        }
      }
    },
    "lineItem": {
      "type": "object",
      "properties": {
        "index": {
          "type": "integer"
        },
        "quantity": {
          "type": "integer"
        },
        "price": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "translatedName": {
          "type": "string"
        },
        "productID": {
          "type": "string"
        },
        "totalPrice": {
          "type": "string"
        },
        "lineItemType": {
          "type": "string"
        },
        "options": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "option": {
                "type": "string"
              },
              "selection": {
                "type": "string"
              }
            }
          }
        },
        "customTextFields": {
          "type": "array",
          "items": {
            "type": "object"
          }
        },
        "weight": {
          "type": "string"
        },
        "mediaItem": {
          "$ref": "product.json#/definitions/image"
        },
        "sku": {
          "type": "string"
        },
        "variantID": {
          "type": "string"
        },
        "discount": {
          "type": "string"
        },
        "tax": {
          "type": "string"
        },
        "taxIncludedInPrice": {
          "type": "boolean"
        },
        "priceData": {
          "$ref": "#/definitions/lineItemPriceData"
        },
        "refundedQuantity": {
          "type": "integer"
        }
      }
    },
    "lineItemPriceData": {
      "type": "object",
      "properties": {
        "taxIncludedInPrice": {
          "type": "boolean"
        },
        "price": {
          "type": "string"
        },
        "totalPrice": {
          "type": "string"
        }
      }
    },
    "trackingInfo": {
      "type": "object",
      "properties": {
        "trackingNumber": {
          "type": "string"
        },
        "shippingProvider": {
          "type": "string"
        },
        "trackingLink": {
          "type": "string"
        }
      }
    }
  }
}