{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Glamaya Open Ai Chat Request",
  "type": "object",
  "properties": {
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "entity_id": {
      "type": "string"
    },
    "entity_type": {
      "$ref": "common-enum-schema.json#/definitions/entity_type"
    },
    "name": {
      "type": "string"
    },
    "prompt": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "template_name": {
      "type": "string"
    },
    "template_version": {
      "type": "string",
      "pattern": "^(\\d+)\\.(\\d+)\\.(\\d+)$"
    },
    "weight": {
      "type": "string"
    }
  },
  "additionalProperties":false,
  "required": ["name", "categories", "tags"]
}