{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Open AI Chat History",
  "type": "object",
  "properties": {
    "template_version": {
      "type": "string"
    },
    "product_id": {
      "type": "integer"
    },
    "sku": {
      "type": "string"
    },
    "chat": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "created": {
          "type": "integer"
        },
        "messages": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/message"
          }
        }
      }
    }

  },
  "additionalProperties":false,
  "required": ["model", "messages"],
  "definitions": {
    "message": {
      "type": "object",
      "javaType": "Message",
      "properties": {
        "role": {
          "$ref": "common-enum-schema.json/#/definitions/role"
        },
        "content": {
          "type": "string"
        }
      },
      "additionalProperties":false,
      "required": [
        "role",
        "content"
      ]
    }
  }
}