{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Wix Product",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 80
    },
    "slug": {
      "type": "string",
      "maxLength": 100
    },
    "visible": {
      "type": "boolean"
    },
    "productType": {
      "type": "string"
    },
    "description": {
      "type": "string",
      "maxLength": 8000
    },
    "sku": {
      "type": "string",
      "maxLength": 40
    },
    "weight": {
      "type": "number",
      "minimum": 0,
      "maximum": 999999999.99
    },
    "stock": {
      "$ref": "#/definitions/stock"
    },
    "priceData": {
      "$ref": "#/definitions/priceData"
    },
    "costRange": {
      "type": "object",
      "properties": {
        "minValue": {
          "type": "integer"
        },
        "maxValue": {
          "type": "integer"
        }
      }
    },
    "additionalInfoSections": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        }
      }
    },
    "media": {
      "$ref": "#/definitions/media"
    },
    "customTextFields": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "maxLength": {
            "type": "integer"
          },
          "mandatory": {
            "type": "boolean"
          }
        }
      }
    },
    "manageVariants": {
      "type": "boolean"
    },
    "productOptions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "optionType": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "choices": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "value": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "media": {
                  "$ref": "#/definitions/media"
                },
                "inStock": {
                  "type": "boolean"
                },
                "visible": {
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
    },
    "productPageUrl": {
      "type": "object",
      "properties": {
        "base": {
          "type": "string"
        },
        "path": {
          "type": "string"
        }
      }
    },
    "numericID": {
      "type": "string"
    },
    "inventoryItemID": {
      "type": "string",
      "format": "uuid"
    },
    "discount": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "value": {
          "type": "integer"
        }
      }
    },
    "collectionIds": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "private Discount discount; "
    },
    "collections": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "variants": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "choices": {
            "type": "object",
            "properties": {
              "size": {
                "type": "string"
              }
            }
          },
          "variantDetail": {
            "type": "object",
            "properties": {
              "priceData": {
                "$ref": "#/definitions/priceData"
              },
              "convertedPriceData": {
                "$ref": "#/definitions/priceData"
              },
              "weight": {
                "type": "number"
              },
              "sku": {
                "type": "string"
              },
              "visible": {
                "type": "boolean"
              }
            }
          },
          "stock": {
            "$ref": "#/definitions/stock"
          }
        }
      }
    },
    "lastUpdated": {
      "type": "string"
    },
    "createdDate": {
      "type": "string"
    },
    "ribbon": {
      "type": "string"
    },
    "brand": {
      "type": "string"
    }
  },
  "definitions": {
    "image": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "mediaId": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "width": {
          "type": "integer"
        },
        "height": {
          "type": "integer"
        },
        "altText": {
          "type": "string"
        },
        "mediaType": {
          "type": "string"
        },
        "fileName": {
          "type": "string"
        }
      }
    },
    "imageInfo": {
      "type": "object",
      "properties": {
        "image": {
          "$ref": "#/definitions/image"
        },
        "imageProvider": {
          "type": "string"
        }
      }
    },
    "priceData": {
      "type": "object",
      "properties": {
        "currency": {
          "type": "string"
        },
        "price": {
          "type": "number"
        },
        "discountedPrice": {
          "type": "number"
        }
      }
    },
    "media": {
      "type": "object",
      "properties": {
        "mainMedia": {
          "$ref": "#/definitions/mainMedia"
        },
        "items": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/mainMedia"
          }
        }
      }
    },
    "mainMedia": {
      "type": "object",
      "properties": {
        "thumbnail": {
          "$ref": "#/definitions/image"
        },
        "mediaType": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "image": {
          "$ref": "#/definitions/image"
        },
        "id": {
          "type": "string"
        }
      }
    },
    "stock": {
      "type": "object",
      "properties": {
        "trackQuantity": {
          "type": "boolean"
        },
        "trackInventory": {
          "type": "boolean"
        },
        "quantity": {
          "type": "integer"
        },
        "inStock": {
          "type": "boolean"
        },
        "inventoryStatus": {
          "type": "string"
        }
      }
    }
  }
}