{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Woocommerce Order",
  "type": "object",
  "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
  "properties": {
    "id": {
      "type": "integer"
    },
    "billing": {
      "$ref": "user.json#/definitions/address"
    },
    "cart_hash": {
      "type": "string"
    },
    "cart_tax": {
      "type": "string"
    },
    "coupon_lines": {
      "type": "array",
      "items": {
        "type": "object",
        "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "code": {
            "type": "string"
          },
          "discount": {
            "type": "string"
          },
          "discount_tax": {
            "type": "string"
          },
          "discount_type": {
            "type": "string"
          },
          "free_shipping": {
            "type": "boolean"
          },
          "meta_data": {
            "$ref": "product.json#/definitions/meta_data"
          },
          "nominal_amount": {
            "type": "number"
          }
        }
      }
    },
    "created_via": {
      "type": "string"
    },
    "customer_id": {
      "type": "string"
    },
    "customer_ip_address": {
      "type": "string"
    },
    "customer_note": {
      "type": "string"
    },
    "currency": {
      "type": "string"
    },
    "currency_symbol": {
      "type": "string"
    },
    "customer_user_agent": {
      "type": "string"
    },
    "date_completed": {
      "type": "string"
    },
    "date_completed_gmt": {
      "type": "string"
    },
    "date_created": {
      "type": "string"
    },
    "date_created_gmt": {
      "type": "string"
    },
    "date_modified": {
      "type": "string"
    },
    "date_modified_gmt": {
      "type": "string"
    },
    "date_paid": {
      "type": "string"
    },
    "date_paid_gmt": {
      "type": "string"
    },
    "discount_tax": {
      "type": "string"
    },
    "discount_total": {
      "type": "string"
    },
    "edit_lock": {
      "type": "boolean",
      "description": "Lock status of the order. Default is 'false'. Applicable only for db order not for woocommerce orders"
    },
    "fee_lines": {
      "type": "array",
      "items": {}
    },
    "is_editable": {
      "type": "boolean"
    },
    "line_items": {
      "type": "array",
      "items": {
        "type": "object",
        "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "image": {
            "$ref": "product.json#/definitions/image"
          },
          "meta_data": {
            "$ref": "product.json#/definitions/meta_data"
          },
          "name": {
            "type": "string"
          },
          "parent_name": {
            "type": "string"
          },
          "price": {
            "type": "integer"
          },
          "product_id": {
            "type": "integer"
          },
          "quantity": {
            "type": "integer"
          },
          "sku": {
            "type": "string"
          },
          "subtotal": {
            "type": "string"
          },
          "subtotal_tax": {
            "type": "string"
          },
          "tax_class": {
            "type": "string"
          },
          "taxes": {
            "$ref": "#/definitions/taxes"
          },
          "total": {
            "type": "string"
          },
          "total_tax": {
            "type": "string"
          },
          "variation_id": {
            "type": "integer"
          }
        }
      }
    },
    "meta_data": {
      "$ref": "product.json#/definitions/meta_data"
    },
    "needs_payment": {
      "type": "boolean"
    },
    "needs_processing": {
      "type": "boolean"
    },
    "number": {
      "type": "string"
    },
    "order_key": {
      "type": "string"
    },
    "parent_id": {
      "type": "integer"
    },
    "payment_method": {
      "type": "string"
    },
    "payment_method_title": {
      "type": "string"
    },
    "payment_url": {
      "type": "string"
    },
    "prices_include_tax": {
      "type": "boolean"
    },
    "refunds": {
      "type": "array",
      "items": {
        "type": "object",
        "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "reason": {
            "type": "string"
          },
          "total": {
            "type": "string"
          }
        }
      }
    },
    "shipping": {
      "$ref": "user.json#/definitions/address"
    },
    "shipping_lines": {
      "type": "array",
      "items": {
        "type": "object",
        "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "instance_id": {
            "type": "string"
          },
          "meta_data": {
            "$ref": "product.json#/definitions/meta_data"
          },
          "method_id": {
            "type": "string"
          },
          "method_title": {
            "type": "string"
          },
          "taxes": {
            "$ref": "#/definitions/taxes"
          },
          "total": {
            "type": "string"
          },
          "total_tax": {
            "type": "string"
          }
        }
      }
    },
    "shipping_tax": {
      "type": "string"
    },
    "shipping_total": {
      "type": "string"
    },
    "status": {
      "$ref": "common-enum-schema.json#/definitions/order_status"
    },
    "tax_lines": {
      "type": "array",
      "items": {
        "type": "object",
        "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "compound": {
            "type": "boolean"
          },
          "label": {
            "type": "string"
          },
          "meta_data": {
            "$ref": "product.json#/definitions/meta_data"
          },
          "rate_code": {
            "type": "string"
          },
          "rate_id": {
            "type": "integer"
          },
          "rate_percent": {
            "type": "number"
          },
          "shipping_tax_total": {
            "type": "string"
          },
          "tax_total": {
            "type": "string"
          }
        }
      }
    },
    "total": {
      "type": "string"
    },
    "total_tax": {
      "type": "string"
    },
    "transaction_id": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "_links": {
      "$ref": "user.json#/definitions/_links"
    }
  },
  "definitions": {
    "taxes": {
      "type": "array",
      "items": {
        "type": "object",
        "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "subtotal": {
            "type": "string"
          },
          "total": {
            "type": "string"
          }
        }
      }
    }
  }
}