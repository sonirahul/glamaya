{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Woocommerce Product Search",
  "type": "object",
  "properties": {
    "after": {
      "type": "string",
      "format": "date"
    },
    "attribute": {
      "type": "string"
    },
    "attribute_term": {
      "type": "string"
    },
    "before": {
      "type": "string",
      "format": "date"
    },
    "category": {
      "type": "integer"
    },
    "exclude": {
      "type": "string"
    },
    "featured": {
      "type": "boolean"
    },
    "include": {
      "type": "string"
    },
    "max_price": {
      "type": "number"
    },
    "min_price": {
      "type": "number"
    },
    "modified_after": {
      "type": "string",
      "format": "date"
    },
    "offset": {
      "type": "integer"
    },
    "on_sale": {
      "type": "boolean"
    },
    "order": {
      "$ref": "#/definitions/order"
    },
    "orderby": {
      "$ref": "#/definitions/orderby"
    },
    "page": {
      "type": "integer"
    },
    "parent": {
      "type": "integer"
    },
    "parent_exclude": {
      "type": "string"
    },
    "per_page": {
      "type": "integer"
    },
    "search": {
      "type": "string"
    },
    "shipping_class": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "status": {
      "$ref": "#/definitions/status"
    },
    "stock_status": {
      "type": "string"
    },
    "tag": {
      "type": "integer"
    },
    "type": {
      "type": "string"
    }
  },
  "definitions": {
    "order": {
      "javaType": "Order",
      "type": "string",
      "enum": [
        "asc",
        "desc"
      ],
      "javaEnums": [
        "ASCENDING",
        "DESCENDING"
      ]
    },
    "orderby": {
      "javaType": "OrderBy",
      "type": "string",
      "enum": ["date", "id", "include", "title", "slug", "modified", "popularity", "rating", "menu_order", "price"]
    },
    "status": {
      "javaType": "Status",
      "type": "string",
      "enum": ["publish", "pending", "draft", "private", "trash"]
    }
  }
}