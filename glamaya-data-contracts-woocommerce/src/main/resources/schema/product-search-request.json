{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Woocommerce Product Search Request to filter Products",
  "type": "object",
  "properties": {
    "after": {
      "type": "string",
      "format": "date"
    },
    "attribute": {
      "type": "string"
    },
    "attribute_term": {
      "type": "string"
    },
    "before": {
      "type": "string",
      "format": "date"
    },
    "category": {
      "type": "integer"
    },
    "exclude": {
      "description": "Comma separated list of ids",
      "type": "string"
    },
    "fetch_latest": {
      "type": "boolean",
      "default": false
    },
    "featured": {
      "type": "boolean"
    },
    "include": {
      "description": "Comma separated list of ids",
      "type": "string"
    },
    "max_price": {
      "type": "number"
    },
    "min_price": {
      "type": "number"
    },
    "modified_after": {
      "type": "string",
      "format": "date"
    },
    "offset": {
      "type": "integer"
    },
    "on_sale": {
      "type": "boolean"
    },
    "order": {
      "$ref": "common-enum-schema.json#/definitions/order"
    },
    "orderby": {
      "$ref": "#/definitions/orderby"
    },
    "page": {
      "type": "integer",
      "default": 1
    },
    "parent": {
      "type": "integer"
    },
    "parent_exclude": {
      "type": "string"
    },
    "per_page": {
      "type": "integer",
      "default": 10
    },
    "search": {
      "type": "string"
    },
    "shipping_class": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "status": {
      "$ref": "common-enum-schema.json#/definitions/status"
    },
    "stock_status": {
      "type": "string"
    },
    "tag": {
      "type": "integer"
    },
    "type": {
      "type": "string"
    },
    "_fields": {
      "type": "string"
    }
  },
  "definitions": {
    "orderby": {
      "javaType": "ProductOrderBy",
      "description": "Represents the field to be sorted by. The \"name\" to be used in sorting from DB, \"value\" to be used to sort from Woo API",
      "enum": ["date", "id", "include", "title", "slug", "modified", "popularity", "rating", "menu_order", "price"],
      "javaEnums": [
        {"name": "date_created"},
        {"name": "id"},
        {"name": "include"},
        {"name": "name"},
        {"name": "slug"},
        {"name": "date_modified"},
        {"name": "popularity"},
        {"name": "rating"},
        {"name": "menu_order"},
        {"name": "price"}
      ],
      "default": "date"
    }
  }
}