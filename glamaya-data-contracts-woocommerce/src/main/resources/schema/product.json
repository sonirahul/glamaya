{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Woocommerce Product",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "attributes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "position": {
            "type": "integer"
          },
          "visible": {
            "type": "boolean"
          },
          "variation": {
            "type": "boolean"
          },
          "options": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    },
    "average_rating": {
      "type": "string"
    },
    "backordered": {
      "type": "boolean"
    },
    "backorders": {
      "type": "string"
    },
    "backorders_allowed": {
      "type": "boolean"
    },
    "button_text": {
      "type": "string"
    },
    "catalog_visibility": {
      "type": "string"
    },
    "categories": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/category"
      }
    },
    "cost_price": {
      "type": "number",
      "title": "Cost Price of a Product"
    },
    "cross_sell_ids": {
      "type": "array",
      "items": {}
    },
    "date_created": {
      "type": "string"
    },
    "date_created_gmt": {
      "type": "string"
    },
    "date_modified": {
      "type": "string"
    },
    "date_modified_gmt": {
      "type": "string"
    },
    "date_on_sale_from": {
      "type": "string"
    },
    "date_on_sale_from_gmt": {
      "type": "string"
    },
    "date_on_sale_to": {
      "type": "string"
    },
    "date_on_sale_to_gmt": {
      "type": "string"
    },
    "default_attributes": {
      "type": "array",
      "items": {}
    },
    "description": {
      "type": "string"
    },
    "dimensions": {
      "type": "object",
      "properties": {
        "length": {
          "type": "string"
        },
        "width": {
          "type": "string"
        },
        "height": {
          "type": "string"
        }
      }
    },
    "download_expiry": {
      "type": "integer"
    },
    "download_limit": {
      "type": "integer"
    },
    "downloadable": {
      "type": "boolean"
    },
    "downloads": {
      "type": "array",
      "items": {}
    },
    "external_url": {
      "type": "string"
    },
    "featured": {
      "type": "boolean"
    },
    "grouped_products": {
      "type": "array",
      "items": {}
    },
    "has_options": {
      "type": "boolean"
    },
    "images": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/image"
      }
    },
    "keywords": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "low_stock_amount": {},
    "manage_stock": {
      "type": "boolean"
    },
    "menu_order": {
      "type": "integer"
    },
    "meta_data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "key": {
            "type": "string"
          },
          "value": {}
        }
      }
    },
    "name": {
      "type": "string"
    },
    "on_sale": {
      "type": "boolean"
    },
    "parent_id": {
      "type": "integer"
    },
    "permalink": {
      "type": "string"
    },
    "price": {
      "type": "number",
      "title": "Auto Calculated Price of a Product",
      "readOnly": true
    },
    "price_html": {
      "type": "string"
    },
    "purchasable": {
      "type": "boolean"
    },
    "purchase_note": {
      "type": "string"
    },
    "rating_count": {
      "type": "integer"
    },
    "regular_price": {
      "type": "number",
      "title": "MRP of a Product"
    },
    "related_ids": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "resale_price": {
      "type": "number",
      "title": "Reselling Price of a Product"
    },
    "reviews_allowed": {
      "type": "boolean"
    },
    "sale_price": {
      "type": "number",
      "title": "Sale Price of a Product"
    },
    "shipping_class": {
      "type": "string"
    },
    "shipping_class_id": {
      "type": "integer"
    },
    "shipping_required": {
      "type": "boolean"
    },
    "shipping_taxable": {
      "type": "boolean"
    },
    "short_description": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "sold_individually": {
      "type": "boolean"
    },
    "status": {
      "$ref": "common-enum-schema.json#/definitions/status"
    },
    "stock_quantity": {
      "type": "integer"
    },
    "stock_status": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/tag"
      }
    },
    "tax_class": {
      "type": "string"
    },
    "tax_status": {
      "type": "string"
    },
    "total_sales": {
      "type": "integer"
    },
    "type": {
      "type": "string"
    },
    "upsell_ids": {
      "type": "array",
      "items": {}
    },
    "variations": {
      "type": "array",
      "items": {}
    },
    "virtual": {
      "type": "boolean"
    },
    "weight": {
      "type": "string"
    }
  },
  "required": ["cost_price", "sku", "resale_price"],
  "definitions": {
    "category": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "count": {
          "type": "integer"
        },
        "description": {
          "type": "string"
        },
        "display": {
          "type": "string"
        },
        "image": {
          "$ref": "#/definitions/image"
        },
        "menu_order": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "parent": {
          "type": "integer"
        },
        "slug": {
          "type": "string"
        }
      }
    },
    "image": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "alt": {
          "type": "string"
        },
        "date_created": {
          "type": "string"
        },
        "date_created_gmt": {
          "type": "string"
        },
        "date_modified": {
          "type": "string"
        },
        "date_modified_gmt": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "src": {
          "type": "string"
        }
      }
    },
    "tag": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "count": {
          "type": "integer"
        },
        "description": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "slug": {
          "type": "string"
        }
      }
    }
  }
}