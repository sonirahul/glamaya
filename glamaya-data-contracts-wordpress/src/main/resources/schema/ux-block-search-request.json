{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Woocommerce UxBlock Search Request to filter UxBlocks",
  "type": "object",
  "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
  "properties": {
    "exclude": {
      "description": "Comma separated list of ids",
      "type": "string"
    },
    "fetch_latest": {
      "type": "boolean",
      "default": false
    },
    "include": {
      "description": "Comma separated list of ids",
      "type": "string"
    },
    "offset": {
      "type": "integer"
    },
    "order": {
      "$ref": "common-enum-schema.json#/definitions/sort_order"
    },
    "orderby": {
      "$ref": "#/definitions/orderby"
    },
    "page": {
      "type": "integer",
      "default": 1
    },
    "parent": {
      "type": "integer"
    },
    "parent_exclude": {
      "type": "string"
    },
    "per_page": {
      "type": "integer",
      "default": 10
    },
    "slug": {
      "type": "string"
    },
    "status": {
      "$ref": "common-enum-schema.json#/definitions/status"
    },
    "_fields": {
      "type": "string"
    }
  },
  "definitions": {
    "orderby": {
      "javaType": "UxBlocksOrderBy",
      "description": "Represents the field to be sorted by. The \"name\" to be used in sorting from DB, \"value\" to be used to sort from Woo API",
      "javaInterfaces" : ["java.io.Serializable", "Cloneable"],
      "enum": ["id", "date", "include", "modified", "parent", "slug", "title"],
      "javaEnums": [
        {"name": "id"},
        {"name": "date"},
        {"name": "include"},
        {"name": "modified"},
        {"name": "parent"},
        {"name": "slug"},
        {"name": "title"}
      ],
      "default": "id"
    }
  }
}