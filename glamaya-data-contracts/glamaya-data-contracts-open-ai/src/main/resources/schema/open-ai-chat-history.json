{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Open AI Chat History",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "entity_id": {
      "type": "string"
    },
    "entity_type": {
      "$ref": "common-enum-schema.json#/definitions/entity_type"
    },
    "history": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/chat"
      }
    }
  },
  "definitions": {
    "chat": {
      "type": "object",
      "properties": {
        "created": {
          "type": "string",
          "format": "date-time"
        },
        "open_ai_chat_request": {
            "$ref": "open-ai-chat-request.json"
        },
        "open_ai_chat_response": {
          "$ref": "open-ai-chat-response.json"
        },
        "template_name": {
          "$ref": "goa-prompt-template.json#/definitions/goa_prompt_template_type"
        },
        "template_version": {
          "type": "string",
          "pattern": "^(\\d+)\\.(\\d+)\\.(\\d+)$"
        }
      },
      "additionalProperties":false
    }
  },
  "additionalProperties":false
}