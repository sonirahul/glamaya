{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Factors for Price Calculator",
  "type": "object",
  "properties": {
    "max_discount_percent": {
      "type": "integer",
      "default": 90,
      "maximum": 90,
      "minimum": 40
    },
    "min_discount_percent": {
      "type": "integer",
      "default": 70,
      "maximum": 90,
      "minimum": 40
    },
    "max_margin_percent": {
      "type": "integer",
      "default": 60
    },
    "min_margin_amount": {
      "type": "integer",
      "default": 150
    },
    "shipping_cost": {
      "type": "integer",
      "default": 180,
      "minimum": 100
    },
    "percent_shipping_in_product_cost": {
      "type": "integer",
      "default": 60,
      "maximum": 70,
      "minimum": 50
    }
  },
  "required": ["max_discount_percent", "min_discount_percent", "max_margin_percent", "min_margin_amount", "shipping_cost", "percent_shipping_in_product_cost"]
}
