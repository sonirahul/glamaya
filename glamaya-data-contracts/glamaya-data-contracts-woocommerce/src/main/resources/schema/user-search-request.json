{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Woocommerce User Search Request to filter Users",
  "type": "object",
  "javaInterfaces": ["java.io.Serializable", "Cloneable"],
  "properties": {
    "_fields": {
      "type": "string"
    },
    "context": {
      "type": "integer"
    },
    "email": {
      "type": "string"
    },
    "exclude": {
      "description": "Comma separated list of ids",
      "type": "string"
    },
    "fetch_latest": {
      "type": "boolean",
      "default": false
    },
    "include": {
      "description": "Comma separated list of ids",
      "type": "string"
    },
    "modified_after": {
      "type": "string",
      "format": "date-time",
      "description": "Limit response to resources published before a given ISO8601 compliant date."
    },
    "offset": {
      "type": "integer"
    },
    "order": {
      "$ref": "common-enum-schema.json#/definitions/sort_order"
    },
    "orderby": {
      "$ref": "#/definitions/orderby"
    },
    "page": {
      "type": "integer",
      "default": 1
    },
    "per_page": {
      "type": "integer",
      "default": 10
    },
    "role": {
      "$ref": "common-enum-schema.json#/definitions/user_role"
    },
    "search": {
      "type": "string"
    }
  },
  "definitions": {
    "orderby": {
      "javaType": "UserOrderBy",
      "description": "Represents the field to be sorted by. The \"name\" to be used in sorting from DB, \"value\" to be used to sort from Woo API",
      "javaInterfaces": ["java.io.Serializable", "Cloneable"],
      "enum": ["id", "include", "name", "registered_date"],
      "javaEnums": [
        {"name": "id"},
        {"name": "include"},
        {"name": "name"},
        {"name": "registered_date"}
      ],
      "default": "name"
    }
  }
}